# ============================================
# Laravel 12 - .gitignore
# ============================================

# Dependencies
/vendor/
node_modules/

# Debug / Log Files
npm-debug.log
yarn-error.log
pnpm-debug.log
*.log
*.cache

# Laravel 4 specific
bootstrap/compiled.php
app/storage/

# Laravel 5+ specific
public/storage
public/hot

# Laravel 5+ with custom public path
public_html/storage
public_html/hot

# Environment & Security
.env
.env.*
!.env.example
.env.backup
.env.production
storage/*.key
auth.json

# Laravel / PHP Specific
.phpunit.result.cache
.phpactor.json
Homestead.yaml
Homestead.json
/.vagrant

# Build artifacts
/public/build
/storage/pail
/vendor/
composer.lock

# IDE / Editor
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# OS Specific
.DS_Store
Thumbs.db

# Node / Frontend Build
/dist
/.vite
/.next
/.nuxt
/.cache
coverage/
resources/js/.vite/
resources/css/.vite/

# Temporary / Backup
*.bak
*.tmp
*.swp
*~
*.orig

# ============================================
# End of Laravel 12 .gitignore
# ============================================
